<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Config
uration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

<package name="javabean" extends="struts-default">

<action name="list" class="javabean.Student" method="getAllStudents">
<result name="success" >/studentlist.jsp</result></action>

<action name="delete" class="javabean.Student" method="delete">
<result name="success" >/studentlist.jsp</result></action>

<action name="add" class="javabean.Student" method="add">
<result name="success" >/studentlist.jsp</result></action>

<action name="edit" class="javabean.Student" method="edit">
<result name="success" >/studentlist.jsp</result></action>

<action name="search" class="javabean.Student" method="search">
<result name="success" >/search.jsp</result></action>



</package>

</struts> 
